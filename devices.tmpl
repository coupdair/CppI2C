<% skin web_skin %>
<% view devices uses content::devices extends master %>
<% template title() %>devices on <% include master::title() %><% end %>
<% template page_content() %>
  <%= content_head | raw %>
  <% foreach dev in device_list %>
    <ul>
    <% item %>
      <li><%= dev %></li>
    <% end %>
    </ul>
  <% end foreach %>
<% end template %> 
<% end view %>
<% end skin %>
